<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devware.disapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:name="com.devware.disapp.PresupuestosDisaApp"
        android:icon="@mipmap/ic_launcher_disa"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_disa"
        android:supportsRtl="true"
        android:theme="@style/Theme.SplashScreen.ComposeSplash">
        <activity
            android:name="com.devware.disapp.ui.view.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.SplashScreen.ComposeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>